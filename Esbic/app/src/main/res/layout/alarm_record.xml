<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical" android:layout_width="match_parent"
    android:layout_height="match_parent">
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="@dimen/width130"
        android:orientation="horizontal">
        <ImageButton
            android:id="@+id/alarm_record_back"
            android:layout_marginLeft="@dimen/width40"
            android:layout_gravity="center"
            android:layout_width="@dimen/width100"
            android:layout_height="@dimen/width100"
            android:background="@drawable/back"/>
        <TextView
            android:layout_gravity="center"
            android:gravity="center"
            android:layout_width="@dimen/width150"
            android:layout_height="@dimen/width100"
            android:textSize="@dimen/textsize30"
            android:text="主页面"/>
        <TextView
            android:layout_gravity="center"
            android:layout_width="@dimen/width200"
            android:gravity="center"
            android:layout_marginLeft="@dimen/width270"
            android:layout_height="@dimen/width100"
            android:textSize="@dimen/textsize40"
            android:text="报警记录"/>
    </LinearLayout>
    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="@dimen/width550">
        <ListView
            android:id="@+id/record_list"
            android:layout_centerHorizontal="true"
            android:layout_centerVertical="true"
            android:layout_width="@dimen/width1200"
            android:layout_height="@dimen/width500"></ListView>
    </RelativeLayout>




</LinearLayout>