<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical" android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/bg_7">
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="@dimen/width93"
        android:background="@drawable/top_bg_2">
        <ImageButton
            android:id="@+id/date_setting_back"
            android:layout_gravity="center"
            android:layout_marginLeft="@dimen/width50"
            android:layout_width="@dimen/width36"
            android:layout_height="@dimen/width81"
            android:background="@drawable/top_back" />
        <TextView
            android:id="@+id/date_setting_back2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center"
            style="@style/text_25_ffffff"
            android:text="返回"/>
    </LinearLayout>

    <ImageView
        android:layout_width="match_parent"
        android:layout_height="@dimen/width8"
        android:background="@drawable/line"/>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="horizontal">
        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="@dimen/width135"
            android:orientation="vertical"
            android:layout_gravity="center">
            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content">
                <TextView
                    android:layout_width="@dimen/width151"
                    android:layout_height="@dimen/width67"
                    android:text="年/月/日"
                    android:gravity="center"
                    android:background="@drawable/words_bg_2"
                    style="@style/text_32_ffffff"/>
                <TextView
                    android:id="@+id/date_year"
                    android:layout_width="@dimen/width126"
                    android:layout_height="@dimen/width68"
                    android:layout_marginLeft="@dimen/width11"
                    android:layout_gravity="center"
                    android:text=""
                    android:background="@drawable/date_cipher_icon"
                    android:gravity="center"
                    android:textSize="@dimen/textsize40"/>

                <TextView
                    android:id="@+id/date_month"
                    android:layout_width="@dimen/width126"
                    android:layout_height="@dimen/width68"
                    android:layout_marginLeft="@dimen/width11"
                    android:layout_gravity="center"
                    android:text=""
                    android:background="@drawable/date_cipher_icon"
                    android:gravity="center"
                    android:textSize="@dimen/textsize40"/>

                <TextView
                    android:id="@+id/date_day"
                    android:layout_width="@dimen/width126"
                    android:layout_height="@dimen/width68"
                    android:layout_marginLeft="@dimen/width11"
                    android:layout_gravity="center"
                    android:text=""
                    android:background="@drawable/date_cipher_icon"
                    android:gravity="center"
                    android:textSize="@dimen/textsize40"/>
            </LinearLayout>
            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/width40">
                <TextView
                    android:layout_width="@dimen/width151"
                    android:layout_height="@dimen/width67"
                    android:text="时/分/秒"
                    android:gravity="center"
                    android:background="@drawable/words_bg_2"
                    style="@style/text_32_ffffff"/>
                <TextView
                    android:id="@+id/date_hour"
                    android:layout_width="@dimen/width126"
                    android:layout_height="@dimen/width68"
                    android:layout_marginLeft="@dimen/width11"
                    android:layout_gravity="center"
                    android:text=""
                    android:background="@drawable/date_cipher_icon"
                    android:gravity="center"
                    android:textSize="@dimen/textsize40"/>
                <TextView
                    android:id="@+id/date_minute"
                    android:layout_width="@dimen/width126"
                    android:layout_height="@dimen/width68"
                    android:layout_marginLeft="@dimen/width11"
                    android:layout_gravity="center"
                    android:text=""
                    android:background="@drawable/date_cipher_icon"
                    android:gravity="center"
                    android:textSize="@dimen/textsize40"/>
                <TextView
                    android:id="@+id/date_second"
                    android:layout_width="@dimen/width126"
                    android:layout_height="@dimen/width68"
                    android:layout_marginLeft="@dimen/width11"
                    android:layout_gravity="center"
                    android:text=""
                    android:background="@drawable/date_cipher_icon"
                    android:gravity="center"
                    android:textSize="@dimen/textsize40"/>
            </LinearLayout>

        </LinearLayout>

        <LinearLayout
            android:layout_width="@dimen/width278"
            android:layout_height="@dimen/width364"
            android:layout_gravity="center"
            android:layout_marginLeft="@dimen/width165"
            android:orientation="vertical">
            <GridView
                android:id="@+id/setting_date_input_keyboard"
                android:layout_gravity="center"
                android:numColumns="3"
                android:horizontalSpacing="@dimen/width25"
                android:verticalSpacing="@dimen/width20"
                android:layout_width="match_parent"
                android:listSelector="#00000000"
                android:layout_height="match_parent"></GridView>

        </LinearLayout>


    </LinearLayout>

</LinearLayout>