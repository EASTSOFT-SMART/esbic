<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical" android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/bg_7">
    <LinearLayout
    android:layout_width="match_parent"
    android:layout_height="@dimen/width93"
    android:background="@drawable/top_bg_2">
    <ImageButton
        android:id="@+id/project_setting_back"
        android:layout_gravity="center"
        android:layout_marginLeft="@dimen/width50"
        android:layout_width="@dimen/width36"
        android:layout_height="@dimen/width81"
        android:background="@drawable/top_back" />
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center"
            style="@style/text_25_ffffff"
            android:text="返回"/>
</LinearLayout>


    <ImageView
        android:layout_width="match_parent"
        android:layout_height="@dimen/width8"
        android:background="@drawable/line"/>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="horizontal">

        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center"
            android:layout_marginLeft="@dimen/width104"
            android:orientation="vertical"
            >

            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:orientation="horizontal">

                <TextView
                    android:layout_width="@dimen/width198"
                    android:layout_height="@dimen/width65"
                    android:background="@drawable/words_bg"
                    style="@style/text_22_ffffff"
                    android:gravity="center"
                    android:text="设备地址"/>
                <TextView
                    android:id="@+id/project_setting_device_address"
                    android:layout_width="@dimen/width497"
                    android:layout_height="@dimen/width66"
                    android:layout_marginLeft="@dimen/width12"
                    android:gravity="center"
                    style="@style/text_22_000000"
                    android:background="@drawable/input_box_3"/>

            </LinearLayout>

            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/width18"
                android:orientation="horizontal">

                <TextView
                    android:id="@+id/ip"
                    android:layout_width="@dimen/width198"
                    android:layout_height="@dimen/width65"
                    android:background="@drawable/words_bg"
                    style="@style/text_22_ffffff"
                    android:gravity="center"
                    android:text="IP"/>

                <TextView
                    android:id="@+id/project_setting_ip"
                    android:layout_width="@dimen/width497"
                    android:layout_height="@dimen/width66"
                    android:layout_marginLeft="@dimen/width12"
                    android:gravity="center"
                    style="@style/text_22_000000"
                    android:background="@drawable/input_box_3"/>


            </LinearLayout>

            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/width18"
                android:orientation="horizontal">

                <TextView
                    android:id="@+id/subnet_mask"
                    android:layout_width="@dimen/width198"
                    android:layout_height="@dimen/width65"
                    android:background="@drawable/words_bg"
                    style="@style/text_22_ffffff"
                    android:gravity="center"
                    android:text="子网掩码 "/>

                <TextView
                    android:id="@+id/project__setting_subnet_mask"
                    android:layout_width="@dimen/width497"
                    android:layout_height="@dimen/width66"
                    android:layout_marginLeft="@dimen/width12"
                    android:gravity="center"
                    style="@style/text_22_000000"
                    android:background="@drawable/input_box_3"/>

            </LinearLayout>

            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/width18"
                android:orientation="horizontal">

                <TextView
                    android:id="@+id/gateway"
                    android:layout_width="@dimen/width198"
                    android:layout_height="@dimen/width65"
                    android:background="@drawable/words_bg"
                    style="@style/text_22_ffffff"
                    android:gravity="center"
                    android:text="网关"/>

                <TextView
                    android:id="@+id/project_setting_gateway"
                    android:layout_width="@dimen/width497"
                    android:layout_height="@dimen/width66"
                    android:layout_marginLeft="@dimen/width12"
                    android:gravity="center"
                    style="@style/text_22_000000"
                    android:background="@drawable/input_box_3"/>
            </LinearLayout>

            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/width18"
                android:orientation="horizontal">

                <TextView
                    android:id="@+id/server"
                    android:layout_width="@dimen/width198"
                    android:layout_height="@dimen/width65"
                    android:background="@drawable/words_bg"
                    style="@style/text_22_ffffff"
                    android:gravity="center"
                    android:text="服务器"/>

                <TextView
                    android:id="@+id/project_server"
                    android:layout_width="@dimen/width497"
                    android:layout_height="@dimen/width66"
                    android:layout_marginLeft="@dimen/width12"
                    android:gravity="center"
                    style="@style/text_22_000000"
                    android:background="@drawable/input_box_3"/>



            </LinearLayout>

            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/width18"
                android:orientation="horizontal">

                <TextView
                    android:id="@+id/center_management"
                    android:layout_width="@dimen/width198"
                    android:layout_height="@dimen/width65"
                    android:background="@drawable/words_bg"
                    style="@style/text_22_ffffff"
                    android:gravity="center"
                    android:text="中心管理机"/>

                <TextView
                    android:id="@+id/project_center_management"
                    android:layout_width="@dimen/width497"
                    android:layout_height="@dimen/width66"
                    android:layout_marginLeft="@dimen/width12"
                    android:gravity="center"
                    style="@style/text_22_000000"
                    android:background="@drawable/input_box_3"/>

            </LinearLayout>

        </LinearLayout>

        <LinearLayout
            android:layout_width="@dimen/width278"
            android:layout_height="@dimen/width364"
            android:layout_gravity="center"
            android:layout_marginLeft="@dimen/width100"
            android:orientation="vertical">
            <GridView
                android:id="@+id/project_setting_input_keyboard"
                android:numColumns="3"
                android:horizontalSpacing="@dimen/width25"
                android:verticalSpacing="@dimen/width20"
                android:layout_width="match_parent"
                android:listSelector="#00000000"
                android:layout_height="match_parent"></GridView>

        </LinearLayout>

    </LinearLayout>

</LinearLayout>