        <LinearLayout
            android:layout_width="@dimen/width278"
            android:layout_height="@dimen/width364"
            android:layout_gravity="center"
            android:layout_marginLeft="@dimen/width100"
            android:orientation="vertical">

            <GridView
                android:id="@+id/project_setting_input_keyboard"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:horizontalSpacing="@dimen/width25"
                android:listSelector="#00000000"
                android:numColumns="3"
                android:verticalSpacing="@dimen/width20"></GridView>

