                        android:clickable="true"
                        android:gravity="center_vertical|left"
                        android:maxLength="3"
                        android:text="255"/>
                </LinearLayout>
            </LinearLayout>

            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/width18"
                android:orientation="horizontal">

                <TextView
                    android:id="@+id/subnet_mask"
                    style="@style/text_22_ffffff"
                    android:layout_width="@dimen/width198"
                    android:layout_height="@dimen/width65"
                    android:background="@drawable/words_bg"
                    android:gravity="center"
                    android:text="子网掩码 "/>

                <LinearLayout
                    android:layout_width="@dimen/width497"
                    android:layout_height="@dimen/width70"
                    android:layout_marginLeft="@dimen/width15"
                    android:background="@color/white"
                    android:orientation="horizontal" >

                    <EditText
                        android:id="@+id/setting_submask_first"
                        android:layout_width="@dimen/width190"
