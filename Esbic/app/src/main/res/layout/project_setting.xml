                        android:text="255"/>

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="."/>

                    <EditText
                        android:id="@+id/setting_center_third"
                        android:layout_width="wrap_content"
