                    <EditText
                        android:id="@+id/setting_device_fourth"
                        android:layout_width="wrap_content"
                        android:layout_height="match_parent"
                        android:layout_weight="1"
                        android:background="@null"
                        android:gravity="center"
                        android:maxLength="2"
                        android:text="05"/>
                    <TextView
                        android:layout_width="@dimen/width130"
                        android:layout_height="match_parent"
                        android:gravity="center_vertical|left"
                        android:text="房间"/>
                </LinearLayout>

            </LinearLayout>

            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/width18"
                android:orientation="horizontal">

                <TextView
                    android:id="@+id/ip"
                    style="@style/text_22_ffffff"
                    android:layout_width="@dimen/width198"
                    android:layout_height="@dimen/width65"
                    android:background="@drawable/words_bg"
                    android:gravity="center"
                    android:text="IP"/>

                <LinearLayout
                    android:layout_width="@dimen/width497"
                    android:layout_height="@dimen/width70"
                    android:layout_marginLeft="@dimen/width15"
                    android:background="@color/white"
                    android:orientation="horizontal" >

                    <EditText
                        android:id="@+id/setting_ip_first"
                        android:layout_width="@dimen/width190"
                        android:layout_height="match_parent"
                        android:layout_weight="1"
                        android:gravity="center_vertical|right"
                        android:background="@null"
